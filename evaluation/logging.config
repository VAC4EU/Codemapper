[loggers]
keys=root

[handlers]
keys=root_stream_handler

[formatters]
keys=root_formatter

[logger_root]
level=DEBUG
handlers=root_stream_handler

[handler_root_stream_handler]
class=StreamHandler
level=DEBUG
formatter=root_formatter
args=(sys.stderr,)

[formatter_root_formatter]
format=%(levelname)-6s %(name)-8s %(message)s
